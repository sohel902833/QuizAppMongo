<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fquiz">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FQuiz"
        android:usesCleartextTraffic="true">
        <activity android:name=".Admin.AdminQuizAnswerListActivity"></activity>
        <activity android:name=".User.QuizActivity" />
        <activity android:name=".User.QuizResultActivity" />
        <activity android:name=".User.RegisterActivity" />
        <activity android:name=".User.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Admin.AdminQuestionActivity" />
        <activity android:name=".Admin.AdminQuizActivity" />
        <activity android:name=".Admin.AdminMainActivity" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>